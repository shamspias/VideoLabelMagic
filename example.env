# SAHI Configuration
SAHI_ENABLED=False
SAHI_MODEL_TYPE=yolov8
SAHI_DEVICE=cpu
SAHI_SLICE_SIZE=256,256
SAHI_OVERLAP_RATIO=0.2,0.2
